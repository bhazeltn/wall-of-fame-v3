<div id="<%= dom_id skater %>">
  <div class="card">
    <div class="card-header">
      <%= skater.name %>
    </div>
    <div class="card-body">
      <div class="card-title">
        <% if skater.active %>
          Active Skater
        <% else %>
          Retired Skater
        <% end %>
      
      </div>
      Reserved for future use

    </div>
  </div>
</div>
